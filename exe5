package main

import "fmt"

/*Crie um Array bidimensional de inteiros com 3 linhas e 2 colunas.
Solicite ao usu√°rio que informe os valores de cada elemento da matriz.
Em seguida, imprima a matriz resultante.*/

func main() {
	var numero int
	var matriz [3][2]int
	//linha 1
	for i := 0; i < 3; i++ {
		fmt.Print("digite um numero:")
		fmt.Scan(&numero)
		matriz[i][0] = numero
	}
	for i := 0; i < 3; i++ {
		fmt.Print("digite um numero:")
		fmt.Scan(&numero)
		matriz[i][1] = numero
	}
	fmt.Println(matriz)
}
