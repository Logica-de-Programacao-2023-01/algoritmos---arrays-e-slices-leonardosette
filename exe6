package main

import "fmt"

/*
Crie um Array de inteiros com 10 elementos.
Em seguida, solicite ao usuário que informe um valor e verifique se esse valor está presente no Array.
Imprima o resultado da busca.
*/
func main() {
	var numero float64
	var lista = [10]float64{1, 2, 3, 4, 5, 6, 7, 8, 9}
	fmt.Print("selecione um numero:")
	fmt.Scan(&numero)
	if numero == lista[9] {
		fmt.Print("sim")
	} else if numero != lista[9] {
		fmt.Print("nao")
	}

}
